<%= form_for(:session, :url => {:action => 'create', :controller => 'sessions'}, :html => {:class => 'well form-inline'} ) do |f| %>
    <%= f.text_field :email, :placeholder => "Email" %>
    <%= f.password_field :password, :placeholder => "Password", :class => 'input-small'   %>
    <%= f.button "Sign in", {:class => 'btn-small'} %>
<% end %>